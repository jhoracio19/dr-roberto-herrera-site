{
  "redirects": [
    {
      "source": "/index.php",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/servicios.php",
      "destination": "/enfermedades",
      "permanent": true
    },
    {
      "source": "/contacto.php",
      "destination": "/#contacto",
      "permanent": true
    },
    {
